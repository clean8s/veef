{
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "preact",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "target": "ESNext",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "declaration": false,
        "module": "ES6",
        "experimentalDecorators": true,
        "types": ["preact/src/jsx"],
    }
}